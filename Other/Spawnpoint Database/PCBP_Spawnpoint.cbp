//<Timer Reset Testfor Clock
FUNC Func_ResetTimerTFClock
	testfor @e[type=ArmorStand,SpawnStand,SPeti>=2<=4]
	SetblockTestfor
	
	@e[type=ArmorStand,SpawnStand,SPeti=2] scoreboard objectives setdisplay sidebar SPetm
	@e[type=ArmorStand,SpawnStand,SPeti=3] scoreboard objectives setdisplay sidebar
	@e[type=ArmorStand,SpawnStand,SPeti=3] SPeti = 1
	
	ScTG @e[type=ArmorStand,SpawnStand,SPeti=4] add SpawnStandReset
	
	USE @e[type=ArmorStand,SpawnStandReset] $ObjR$
		SPeti
		SPetm
		SPetc
		SPafk
		SPafkc
	UNUSE
	
	USE @e[type=ArmorStand,SpawnStandReset] $ObjA$
		SPeti dummy Spawnpoint Event Timer - Ticks
		SPetm dummy Spawnpoint Event Timer - Mins
		SPetc dummy Spawnpoint Event Timer - Calc
		SPafk dummy Spawnpoint AFK Toggle
		SPafkc dummy Spawnpoint AFK Toggle - Calc
	UNUSE
	
	@e[type=ArmorStand,SpawnStandReset] SPeti = 0
	ScTG @e[type=ArmorStand,SpawnStandReset] remove SpawnStandReset
	
	@e[type=ArmorStand,SpawnStand,SPeti>=2<=4] SPeti = 0
END
