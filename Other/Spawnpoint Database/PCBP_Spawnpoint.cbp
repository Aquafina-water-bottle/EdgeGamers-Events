//<Other Clock
FUNC Func_OtherClock
///	Dimension
	@a SPdim = 0
	@a SPdim = 6 {$Dim$}

///	Mastermind
	@a[$MM$,SPdim=6] SPmm = 1
	@a[$NotMM$,SPmm=1] SPmm = 2
	ScTE Mastermind SPmm 0 0
	CONDITION
		@a[SPmm=1] SPmm = 2
	$s$clear @a[SPmm=2] wool -1 -1 {display:{Lore:["Mastermind Wool"]}}
	$s$clear @a[SPmm=2] shears -1 -1 {display:{Lore:["Mastermind Shears"]}}
	@a[SPmm=2] SPmm = 0
	
///	PVP
	@e[type=ArmorStand,SpawnStand,SPpvp=0] ScTJ NoPVP @a[team=!NoPVP,SPdim=6]
	
///	Saturation
	@e[type=ArmorStand,SpawnStand,SPsat=1] effect @a[SPdim=6] 23 2 0 true
	
///	Weather
	@e[type=ArmorStand,SpawnStand,SPwea=0] $s$weather clear 1000000
	@e[type=ArmorStand,SpawnStand,SPwea=1] $s$weather rain 1000000
	@e[type=ArmorStand,SpawnStand,SPwea=2] $s$weather thunder 1000000
	
///	Regular gamemode
	@e[type=ArmorStand,SpawnStand,SPgmd=1] $s$gamemode 2 @a[m=0,SPdim=6]
	
///	Affected players gamemode
	@a MCTeams + 0
	@a[MCTeams=0] MCTeams = 1
	
//=
SPaff
	OT		1
	APPS	2
	AT		4
	EC		8
	LE		16
	
	Normal settings: 1 + 2 = 3

SPst	Specialty teams		Players
	OT = 1
	APPS = 2
	AT = 4
	EC = 8 
	LE = 16
\\=

///	Calculating exceptions
	ScOP @e[type=ArmorStand,SpawnStand,SPaff>=1] SPafc = @e[type=ArmorStand,SpawnStand,SPaff>=1] SPaff
	@e[type=ArmorStand,SpawnStand,SPafc>=16] @a[MCTeams=16,SPdim=6] SPafc = 1
	@e[type=ArmorStand,SpawnStand,SPafc>=16] SPafc - 16
	@e[type=ArmorStand,SpawnStand,SPafc>=8] @a[MCTeams=8,SPdim=6] SPafc = 1
	@e[type=ArmorStand,SpawnStand,SPafc>=8] SPafc - 8
	@e[type=ArmorStand,SpawnStand,SPafc>=4] @a[MCTeams=4,SPdim=6] SPafc = 1
	@e[type=ArmorStand,SpawnStand,SPafc>=4] SPafc - 4
	@e[type=ArmorStand,SpawnStand,SPafc>=2] @a[MCTeams=2,SPdim=6] SPafc = 1
	@e[type=ArmorStand,SpawnStand,SPafc>=2] SPafc - 2
	@e[type=ArmorStand,SpawnStand,SPafc>=1] @a[MCTeams=1,SPdim=6] SPafc = 1
	@e[type=ArmorStand,SpawnStand,SPafc>=1] SPafc = 0
	
///	Gamemode
	$s$gamemode 2 @a[m=1,SPafc=1,SPdim=6]
	$s$gamemode 2 @a[m=0,SPafc=1,SPdim=6]
///	Leave Game Teleport
	ScOP @a[SPafc=1,SPlg>=1,SPdim=6] SPtp = spawn SPna
	
	@a[SPafc=1] SPafc = 0
	@a[SPlg>=1] SPlg = 0
END
