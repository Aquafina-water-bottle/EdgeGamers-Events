//<SpawnPoint Testfor Clock
FUNC Func_SpawnpointTFClock
	testfor @a[SPid>=1]
	SetblockTestfor
	
	LOOP 1 $Num$ + 1
		spawnpoint @a[SPid=|0|] $Coords|0|$
		
		//-
		tellraw @a[SPid=|0|] $TextStart$
		{"text":"Your spawnpoint has been set to ","color":"gray"},
		{"text":"$Coords|0|$","color":"yellow","bold":"true"},
		{"text":" at ","color":"gray"},
		{"text":"$Event|0|$","color":"$EventC|0|$","bold":"true",
			"clickEvent":{"action":"run_command","value":"/scoreboard players set @p SPtp |0|"},
			"hoverEvent":{"action":"show_text","value":{"text":"Teleport to $Event|0|$","color":"$EventC|0|$"}}
		},
		{"text":"!","color":"gray"}]}
		\\-
	END
	
	@a[SPid>=1] playsound minecraft:entity.experience_orb.touch voice @a[c=1,SPid>=1]
	ScOP @a[SPid>=1] SPid *= -1 Number
END
