//<General Scoreboard Start
FUNC Func_GeneralScoreboardStart
	USE $ObjA$
		NumberObjAdd
		MM dummy Mastermind
		MMrs dummy MasterMind Room Select
		MMrn dummy MasterMind Room Number
		MMgn dummy MasterMind Guess Number
		MMpa dummy MasterMind Predicted Answer
		MMca dummy MasterMind Correct Answer
		MMsd dummy MasterMind Sign Display
		MMra dummy MasterMind Repeated Answer
		MMna dummy MasterMind No Answer
		MMst stat.sneakTime MasterMind Sneak Timer
		MMgc dummy MasterMind Guess Check
		MMgl dummy MasterMind Guess Calc
		MMit dummy MasterMind Item
		MMti dummy MasterMind Timer
		MMop dummy Mastermind Options
	UNUSE
	
	ExecuteSpawnStand scoreboard objectives setdisplay sidebar MM
	
	USE $TeamA$
		MMred
		MMlime
		MMcyan
		MMmagenta
		MMpink
		MMorange
	UNUSE
	
	USE $TeamO$
		MMred color red
		MMlime color green
		MMcyan color dark_aqua
		MMmagenta color dark_purple
		MMpink color light_purple
		MMorange color gold
	UNUSE
	
	NumberAdd(-1)
	
///	Summon Room Armor Stands
	ExecuteSpawnStand summon ArmorStand ~ ~ ~ {Tags:["MMEntity","MMStand"],Invulnerable:1,PersistenceRequired:1,Invisible:1,Marker:1,NoGravity:1}
	LOOP 1 6 + 1 () 0 55 + 11
		ExecuteSpawnStand summon ArmorStand ADD($RoomNX$;|2|) $RoomNY$ $RoomNZ$ ;
		{Tags:["MMEntity","MMRoom","MMRoom|0|"],Invulnerable:1,PersistenceRequired:1,Invisible:1,Marker:1,NoGravity:1}
	END
	
	LOOP 7 12 + 1 () 0 55 + 11
		ExecuteSpawnStand summon ArmorStand ADD($RoomSX$;|2|) $RoomSY$ $RoomSZ$ ;
		{Tags:["MMEntity","MMRoom","MMRoom|0|"],Invulnerable:1,PersistenceRequired:1,Invisible:1,Marker:1,NoGravity:1}
	END
	
	LOOP 1 12 + 1
		ExecuteSpawnStand @e[type=ArmorStand,MMRoom|0|] MMrs = -|0|
	END
	
	ExecuteSpawnStand @e[type=ArmorStand,MMRoom] ScOP @e[c=1,r=1,type=ArmorStand,MMRoom] MMrn = @e[c=1,r=1,type=ArmorStand,MMRoom] MMrs
	ExecuteSpawnStand ScOP @e[type=ArmorStand,MMRoom] MMrn *= -1 Number
	ExecuteSpawnStand @e[type=ArmorStand,MMRoom] MMgc = 1
	ExecuteSpawnStand @e[type=ArmorStand,MMRoom] MMgl = 0
	
///	Summon Correct Answer Armor Stands
	LOOP 1 4 + 1 () 0 3 + 1
		ExecuteSpawnStand summon ArmorStand SUB($MMCAx$;|2|) $MMCAy$ $MMCAz$ ;
		{Tags:["MMEntity","MMCA","MMCA|0|"],DisabledSlots:2096896,NoGravity:1,NoBasePlate:1,ShowArms:1,Small:1,Invulnerable:1,PersistenceRequired:1,$CustomAS|0|$}
	END
	
///	Spawnpoint clock
	Mastermind SPmm = 1
END
