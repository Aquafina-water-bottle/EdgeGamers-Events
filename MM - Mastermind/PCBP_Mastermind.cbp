	@e[type=ArmorStand,MMStand,MMop>=2048] fill $FillDoorsN$ air 0 replace bedrock 0
	@e[type=ArmorStand,MMStand,MMop>=2048] fill $FillDoorsS$ air 0 replace bedrock 0
	
