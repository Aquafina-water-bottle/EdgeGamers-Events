	CONDITION
		Activate(CBPRandomizerPulse)
	
	testfor @e[type=ArmorStand,MMRefresh,MMti=10]
	CONDITION
		Activate(CBPSetButtonsPulse)
	@e[type=ArmorStand,MMRefresh,MMti>=10] MMti = -1
END
